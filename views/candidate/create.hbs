<div class="col-md-offset-3 col-md-6 col-md-offset-3">
<form class="form-horizontal" method="post" action="create">
  <div class="form-group">
    <div class="col-sm-10">
      <input type="text" id="name" name="name" placeholder="Name" class="form-control">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <select name="region" id="region" class="form-control">
        <option value="">--SELECT--</option>
        {{#if regions}}
          {{#each regions}}
            <option value="{{dataValues.id}}">{{dataValues.name}}</option>
          {{/each}}
        {{/if}}
      </select>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <select name="position" id="position" class="form-control">
        <option value="">--SELECT--</option>
        <option value="president">President</option>
        <option value="Governor">Governor</option>
        <option value="Senator">Senator</option>
        <option value="MP">MP</option>
        <option value="MCA">MCA</option>
        <option value="Women Rep">Women Rep</option>
      </select>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <input type="text" id="odd" name="odd" placeholder="Odd" class="form-control">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <input type="text" id="probability" name="probability" placeholder="0 to 100" class="form-control">
    </div>
  </div>
  <div class="form-group">
    <div class="">
      <button type="submit" class="button btn btn-warning">Create</button>
    </div>
  </div>
</form>
</div>