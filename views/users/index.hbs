<h1>{{title}}</h1>

<div class="panel panel-warning">
	<div class="panel panel-body">
		<table class="table table-hover candidates">
			<thead>
				<th>ID</th>
				<th>Username</th>
				<th>Full Names</th>
				<th>Role</th>
				<th>Credits</th>
				<th>Created At</th>
			</thead>
			<tbody>
			{{#if users}}
				{{#each users}}
					<tr>
						<td>{{dataValues.id}}</td>
						<td>{{dataValues.username}}</td>
						<td>{{dataValues.names}}</td>
						<td>{{dataValues.role}}</td>
						<td>{{dataValues.credits}}</td>
						<td>{{dataValues.createdAt}}</td>
					</tr>
				{{/each}}
			{{/if}}
			</tbody>
		</table>
	</div>
</div>